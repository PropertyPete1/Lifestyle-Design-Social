{
  "compilerOptions": {
    "target": "es2018",
    "module": "commonjs",
    "rootDir": "./",
    "outDir": "./dist",
    "downlevelIteration": true,
    "allowSyntheticDefaultImports": true,
    "esModuleInterop": true,
    "forceConsistentCasingInFileNames": true,
    "strict": false,
    "skipLibCheck": true,
    "noImplicitAny": false,
    "strictNullChecks": false
  },
  "include": ["app.ts", "src/**/*"]
}